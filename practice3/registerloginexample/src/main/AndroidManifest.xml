<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jh.registerloginexample">

    <uses-permission android:name="android.permission.INTERNET"/> <!--인터넷 권한 선언-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practice">
        <activity android:name=".LoginActivityJAVA"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter><!-- intent filter 있는쪽이 가장 먼저 열림림-->
        </activity>
        <activity android:name=".RegisterActivityJAVA"
            android:exported="true"/>
        <activity android:name=".MainActivityJAVA"
            android:exported="true">

        </activity>
    </application>

</manifest>